{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "types": ["vue"],
    "lib": ["dom", "esnext"],
    "jsx": "preserve",
    "sourceMap": true,
    "allowJs": true,
    "esModuleInterop": true,
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.vue"],
  "exclude": ["node_modules", "dist"],
  "files": [],
  // "references": [
  //   {
  //     "path": "./tsconfig.node.json"
  //   },
  //   {
  //     "path": "./tsconfig.app.json"
  //   }
  // ]
}
